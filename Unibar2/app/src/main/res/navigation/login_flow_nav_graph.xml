<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/profileInfoFragment">
    <fragment
        android:id="@+id/signUpFragment"
        android:name="com.group25.unibar.Fragments.SignUpFragment"
        android:label="fragment_sign_up"
        tools:layout="@layout/activity_sign_up" />
    <fragment
        android:id="@+id/signInFragment"
        android:name="com.group25.unibar.Fragments.SignInFragment"
        android:label="fragment_sign_in"
        tools:layout="@layout/activity_sign_in">
        <action
            android:id="@+id/action_signInFragment_to_signUpActivity2"
            app:destination="@+id/signUpFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileInfoFragment"
        android:name="com.group25.unibar.Fragments.ProfileInfoFragment"
        android:label="fragment_profile_info"
        tools:layout="@layout/fragment_profile_info" />
</navigation>